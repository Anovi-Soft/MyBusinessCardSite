@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width"/>
    <script type="text/javascript" src="~/Scripts/Initializer.js"></script>
    <link rel="stylesheet" type="text/css" href="~/Content/StyleFonts.css"/>
    <link rel="stylesheet" type="text/css" href="~/Content/StyleBase.css"/>
    @if (Request.Browser.IsMobileDevice)
    {
        <link rel="stylesheet" type="text/css" href="~/Content/StyleMobile.css" />
    }
    else
    {
        <link rel="stylesheet" type="text/css" href="~/Content/StyleSite.css" />
    }
    <link rel="shortcut icon" href="~/Content/Images/icon.png">
    <title>AnoviSoft</title>
</head>
<body id="baseBody" onload="init()">

<div class="center_column" id="baseColumn">
    <div class="row paper_group">
        <div class="col-1-3">
            <img class="photo" src="~/Content/Images/avatar.jpg" alt="Novikov Andrey"/>
        </div>
        <div class="col-2-3">
            <div class="paper">
                <div class="paper_up">
                    <h1>Novikov Andrey Igorevich</h1>
                </div>

                <h3>DevSkills</h3>
                <br/>
                <h4>С++</h4>
                <ul>
                    <li>High standard of knowledge</li>
                    <li>Standard C ++ 14</li>
                    <li>MFC</li>
                </ul>
                <h4>С#</h4>
                <ul>
                    <li>High standard of knowledge</li>
                    <li>C# 6.0</li>
                    <li>.net Framework 4.6</li>
                    <li>WPF</li>
                </ul>
                <h4>Python</h4>
                <ul>
                    <li>Python 3.x</li>
                </ul>
            </div>
        </div>
    </div>
    <div class="row paper_group">
        <div class="col-1">
            <div class="paper">
                <div class="paper_up">
                    <h2>Development of desktop and server applications</h2>
                </div>
                <ul>
                    <li>Windows </li>
                    <li>OS X</li>
                    <li>Linux</li>
                </ul>
            </div>
        </div>
        <div class="col-1-2">
            <div class="paper">
                <div class="paper_up">
                    <h2>Game moding</h2>
                </div>
                <ul>
                    <li>Unity engine</li>
                    <li>Unreal engine</li>
                </ul>
                <h3>Wide experience of a moding:</h3>
                <div>Kerbal Space Program</div>
            </div>
        </div>
        <div class="col-1-2">
            <div class="paper">
                <div class="paper_up">
                    <h2>Dev mobile applications</h2>
                </div>
                <h3>Crossplatform or not</h3>
                <ul>
                    <li>Android </li>
                    <li>IOS</li>
                    <li>WinFon</li>
                </ul>
            </div>
        </div>
    </div>


</div>

</body>
</html>
